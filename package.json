{
  "name": "@thx/money",
  "version": "1.1.0",
  "description": "Money addons",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.babel.js",
    "demo": "react-scripts start",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "flow": "flow",
    "lint": "eslint *.js src/lib",
    "docs": "jsdoc -c jsdoc.json",
    "docs:md": "jsdoc2md -c jsdoc.json -f src/lib/*.js > docs/README.md",
    "docs:explain": "jsdoc -X -c jsdoc.json",
    "validate": "npm ls"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/money.git"
  },
  "keywords": [
    "react",
    "controls",
    "money",
    "currency",
    "js-money"
  ],
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.1",
    "flow-bin": "^0.49.1",
    "ink-docstrap": "^1.3.0",
    "jest": "^20.0.4",
    "jsdoc": "^3.5.4",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "precommit-hook-eslint": "^3.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10",
    "react-test-renderer": "^15.6.1",
    "semantic-ui-react": "^0.71.4",
    "webpack": "^3.5.4",
    "webpack-node-externals": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^15.5.0",
    "semantic-ui-react": "^0.71.4"
  },
  "dependencies": {
    "@thx/tproptypes": "^1.0.0",
    "accounting": "^0.4.1",
    "debug": "^3.0.0",
    "inputmask": "^3.3.7",
    "js-money": "^0.6.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10"
  },
  "pre-commit": [
    "lint",
    "flow",
    "test",
    "docs:md"
  ]
}
